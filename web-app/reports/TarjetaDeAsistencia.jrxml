	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA_INI" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ID" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT E.id,E.clave,CONCAT(E.apellido_paterno," ",E.apellido_materno," ",E.nombres) AS nombre,
P.numero_de_trabajador,P.jornada
,(SELECT U.descripcion FROM ubicacion U WHERE U.ID=P.ubicacion_id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE U.ID=P.departamento_id) AS depto
,A.ID,A.periodo_fecha_inicial,A.periodo_fecha_final,A.incapacidades,A.vacaciones
,A.retardo_menor,A.retardo_mayor,A.retardo_comida
,(case when dayofweek(fecha)=1 then 'DOM' when dayofweek(fecha)=2 then 'LUN' when dayofweek(fecha)=3 then 'MAR' when dayofweek(fecha)=4 then 'MIE' when dayofweek(fecha)=5 then 'JUE' when dayofweek(fecha)=6 then 'VIE' when dayofweek(fecha)=7 then 'SAB' ELSE 'ND' END) as diaSem
,D.id,d.fecha,D.entrada1,d.salida1,d.entrada2,d.salida2,d.entrada3,d.salida3,d.comentario
,ifnull(((TIME_TO_SEC((CASE WHEN D.SALIDA2 IS NULL THEN SEC_TO_TIME((TIME_TO_SEC(d.salida1)-TIME_TO_SEC(d.entrada1))) ELSE SEC_TO_TIME((TIME_TO_SEC(d.salida2)-TIME_TO_SEC(d.entrada1))) END)))/60)/60,0) AS hrsTrab
,ifnull(((TIME_TO_SEC((case when d.salida2 is null then d.salida1 else d.salida2 end))-TIME_TO_SEC((case when d.salida2 is null then '14:00:00' else '18:30:00' end)))/60),0) AS tpoExc
,(CASE WHEN ifnull(((TIME_TO_SEC(d.entrada1)-TIME_TO_SEC('09:00:00'))/60),0)<0 THEN 0 WHEN ifnull(((TIME_TO_SEC(d.entrada1)-TIME_TO_SEC('09:00:00'))/60),0)<=10 THEN ifnull(((TIME_TO_SEC(d.entrada1)-TIME_TO_SEC('09:00:00'))/60),0) ELSE 0 END) AS retMn
,(CASE WHEN ifnull(((TIME_TO_SEC(d.entrada1)-TIME_TO_SEC('09:00:00'))/60),0)>10 THEN ifnull(((TIME_TO_SEC(d.entrada1)-TIME_TO_SEC('09:00:00'))/60),0) ELSE 0 END) AS retMy
,(CASE WHEN (ifnull(((TIME_TO_SEC(d.entrada2)-TIME_TO_SEC(d.salida1))/60),0)-60)<0 THEN 0 ELSE (ifnull(((TIME_TO_SEC(d.entrada2)-TIME_TO_SEC(d.salida1))/60),0)-60) END) AS retCm
FROM asistencia A 
JOIN perfil_de_empleado P ON(a.empleado_id=P.empleado_id)
JOIN empleado E ON(A.empleado_id=E.ID)
JOIN asistencia_det D ON(A.ID=D.asistencia_id)
WHERE A.ID=$P{ID}  
 ]]></queryString>

