	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="CAL_ID_INI" isForPrompting="true" class="java.lang.String"/>
	<parameter name="CAL_ID_FIN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_INI_PAGO" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FIN_PAGO" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT E.id,E.clave,CONCAT(ifnull(E.apellido_paterno,"")," ",ifnull(E.apellido_materno,"")," ",E.nombres) AS nombre,P.numero_de_trabajador,P.jornada,a.tipo as tipoPrdo
,(SELECT min(c.folio)  FROM calendario_det c where c.id BETWEEN $P{CAL_ID_INI} and $P{CAL_ID_FIN} ) as folio_ini
,(SELECT max(c.folio)  FROM calendario_det c where c.id BETWEEN $P{CAL_ID_INI} and $P{CAL_ID_FIN} ) as folio_fin
,(SELECT min(c.asistencia_fecha_inicial)  FROM calendario_det c where c.id BETWEEN $P{CAL_ID_INI} and $P{CAL_ID_FIN} ) as periodo_fecha_inicial
,(SELECT max(c.asistencia_fecha_final)  FROM calendario_det c where c.id BETWEEN $P{CAL_ID_INI} and $P{CAL_ID_FIN} ) as periodo_fecha_final
,(SELECT U.descripcion FROM ubicacion U WHERE U.ID=P.ubicacion_id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE U.ID=P.departamento_id) AS depto
,D.id,d.fecha,D.entrada1,d.salida1,d.entrada2,d.salida2,d.entrada3,d.salida3,d.comentario
,d.retardo_menor,(case when d.retardo_menor>0 then 1 else 0 end) as regs_ret_min
,d.retardo_menor_comida as ret_men_com,(case when d.retardo_Menor_comida>0 then 1 else 0 end) as regs_ret_men_com
FROM EMPLEADO E 
JOIN perfil_de_empleado P ON(E.ID=P.empleado_id)
JOIN asistencia A ON(A.empleado_id=E.ID)
JOIN asistencia_det D ON(A.ID=D.asistencia_id)
WHERE A.empleado_id =$P{ID} AND a.calendario_det_id between $P{CAL_ID_INI} and $P{CAL_ID_FIN}]]></queryString>

